<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="com.smt.hibernate.LoginEditBillHibernate" table="edit_bill_login_user_details">

		<id name="pkLoginEditBillId" type="java.lang.Long" column="pk_login_edit_bill_id">
			<generator class="increment" />
		</id>

		<property name="password" type="java.lang.String" column="password"	length="255" />
		<property name="shopId" type="java.lang.Long" column="fk_shop_id" />
	</class>
</hibernate-mapping>